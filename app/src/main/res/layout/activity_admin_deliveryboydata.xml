<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".Admin.AdminDeliveryboydataActivity">
    <androidx.appcompat.widget.SearchView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/svAdmingetShopkeeperlistSearchView"
        app:queryHint="Search Shopkeeper"
        app:iconifiedByDefault="false"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="16sp"
        android:layout_marginStart="16sp"
        android:background="@drawable/shape"
        />
    <ListView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginEnd="16sp"
        android:layout_marginStart="16sp"
        android:layout_marginTop="16sp"
        android:id="@+id/lvAdmingetalldeliveryDatalist"
        />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="No User Avaiable"
        android:id="@+id/tvAdmingetAlldeliverynouseravaiable"
        android:textSize="26sp"
        android:textColor="@color/gray"
        />


</LinearLayout>