<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ShopkeeperhomeActivity">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/FrameLayoutshopkeeperhome"
        android:layout_above="@id/bottomnevigatiomshopkeeprhome"
        />
    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:layout_width="match_parent"
        android:layout_height="106dp"
        android:id="@+id/bottomnevigatiomshopkeeprhome"
        app:menu="@menu/shopkeeper_bottomnevigation"
        android:layout_alignParentBottom="true"
        app:itemIconTint="@color/nevybluecolor"
        app:itemActiveIndicatorStyle="@style/App.Custom.Indicator"
        app:itemTextColor="@color/nevybluecolor"
        android:background="@color/white"
        />

</RelativeLayout>